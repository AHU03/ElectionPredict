<?xml version="1.0" encoding="utf-8" ?>
<classes:CustomShell  xmlns:classes="clr-namespace:ElectionPredictFinal.Pages.Classes"
                      xmlns="http://xamarin.com/schemas/2014/forms"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:ElectionPredictFinal"
       xmlns:local1="clr-namespace:ElectionPredictFinal.Pages"
       x:Class="ElectionPredictFinal.Pages.AppShellPage"
       Shell.NavBarIsVisible="False">

    <TabBar>
        <Tab Title="Modell" Icon="Vorhersagemodell_icon.png">
            <ShellContent x:Name="PredicitonModelTab" Shell.TabBarBackgroundColor="#151515" Shell.TabBarTitleColor="FloralWhite"/>
        </Tab>
        <Tab Title="Partei-Parolen" Icon="Partei-Parolen_icon.png">
            <ShellContent x:Name="PartyInfluenceTab" Shell.TabBarBackgroundColor="#151515" Shell.TabBarTitleColor="FloralWhite"/>
        </Tab>
        <Tab Title="Wahldaten" Icon="Wahlresultate_icon.png">
            <ShellContent x:Name="DataViewerTab" Shell.TabBarBackgroundColor="#151515" Shell.TabBarTitleColor="FloralWhite"/>
        </Tab>
    </TabBar>
</classes:CustomShell>